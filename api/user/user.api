syntax = "v1"

info(
	title: "用户api"
	desc: "用户相关api"
	author: "xu756"
	email: "756334744@qq.com"
	version: "v1"
)

type (
	userLogin {
		Username string `json:"username"`
		Password string `json:"password"`
	}
	Res {
		Code int         `json:"code"`
		Msg  string      `json:"msg"`
		Data interface{} `json:"data"`
	}
)
@server(
	prefix: api/user/v1
	jwt: Auth
)
service user-api {
	@doc "用户登录"
	@handler userLogin
	post /login (userLogin) returns (Res)
}